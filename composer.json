{
  "name": "sysbot/bin",
  "description": "Basic binary serialization/de-serialization utilities, providing support for both 32-bit and 64-bit PHP versions.",
  "keywords": [
    "serialize",
    "serialization",
    "deserialization",
    "binary",
    "pack",
    "unpack"
  ],
  "license": "LGPL-3.0-or-later",
  "require": {
    "composer-runtime-api": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "optimize-autoloader": true
  },
  "suggest": {
    "brick/math": "Required on 32-bit systems for correctly encoding/decoding binary data.",
    "ext-bcmath": "Useful for speeding up calculations on 32-bit systems."
  },
  "autoload": {
    "psr-4": {
      "Sysbot\\Bin\\": "src/"
    },
    "files": [
      "src/init.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sysbot\\Tests\\Bin\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "sys-001",
      "email": "sys@sys001.ml",
      "homepage": "https://sys001.ml",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Sysbot-org/bin/issues"
  }
}
